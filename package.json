{
  "name": "module",
  "version": "0.0.1",
  "devDependencies": {
    "browserify": "^13.0.0",
    "uglify-js": "^2.6.1"
  },
  "scripts": {
    "browserify": "node_modules/browserify/bin/cmd.js module.js > build/module.js",
    "uglify": "node_modules/uglify-js/bin/uglifyjs build/module.js -o build/module.min.js",
    "build": "rm build/* && npm run browserify && npm run uglify"
  }
}
